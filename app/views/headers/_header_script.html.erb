  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"></script>
  <!-- Input for tooltip --> <script type="text/javascript" src="http://cdn.jquerytools.org/1.2.5/jquery.tools.min.js"></script>
  <!-- Input for widget --> <script type="text/javascript" src="/colorbox/jquery.colorbox.js"></script>
  <!-- Input for widget --> <link rel="stylesheet" media="screen" type="text/css" href="/colorbox/colorbox.css"  />
  
  <!-- Input for slide tab -->
  <script type="text/javascript" src="/tab_bar/scripts/prototype.lite.js"></script>
  <script type="text/javascript" src="/tab_bar/scripts/moo.fx.js"></script>
  <script type="text/javascript" src="/tab_bar/scripts/moo.fx.pack.js"></script>
  <link rel="stylesheet" media="screen" type="text/css" href="/tab_bar/tab_style.css"  />
  <!-- Input for slide tab -->
  
  <script type="text/javascript">
    // Input for slide tab
    function init(){
    var stretchers = document.getElementsByClassName('box');
    var toggles = document.getElementsByClassName('tab');
    var myAccordion = new fx.Accordion(
      toggles, stretchers, {opacity: false, height: true, duration: 600}
    );
    //hash functions
    var found = false;
    toggles.each(function(h3, i){
      var div = Element.find(h3, 'nextSibling');
        if (window.location.href.indexOf(h3.title) > 0) {
          myAccordion.showThisHideOpen(div);
          found = true;
        }
      });
    if (!found) myAccordion.showThisHideOpen(stretchers[0]);
    }
    // End of input for slide tab
    
    jQuery(document).ready(function() { 
      // Input for widget
      jQuery("#open_colorbox").colorbox({width:"40%", height:"80%", iframe:true});
      // End of input for widget
      
      // Input for tooltip
      jQuery("[title]").tooltip({
      	// place tooltip on the right edge
	    position: "center right",
	    // a little tweaking of the position
    	offset: [-2, 10],
	    // use the built-in fadeIn/fadeOut effect
	    effect: "fade",
	    // custom opacity setting
	    opacity: 0.7   
      });
      // End of input for tooltip
    });
  </script>
  
  <!-- Style for widget -->
  <style type="text/css">
    #widget {
	  position:fixed;
	  top:250px;
	  right: 0px;
	  }
  </style>
  <!-- End of style for widget -->
  </script>


